  <body>
    <% include ../partials/nav %> <!-- 2 -->

    <div class="container contact contact-index">
      <h2>Index</h2>
      <ul class="list-group">
        <table class="type02" >
          <colgroup>
            <col width="5%" />
            <col width="5%" />
            <col width="5%" />
            <col width="5%" />
            <col width="5%" />
