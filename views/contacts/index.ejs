          <tbody>
            <!---
              <% for(var i=0; i<rows.length; i++) { %>
              <tr>
                <td><a href=<% rows[i]._id %></a><%= rows[i].IDX %></td>
                <td><%= rows[i].RSD %></td>
                <td><%= rows[i].IA %></td>
                <td><%= rows[i].RSU %></td>
                <td><%= rows[i].SEQ %></td>
                <td><%= rows[i].REGDATE %></td>
                <td><%= rows[i].CO2 %></td>
                <td><%= rows[i].RH %></td>
                <td><%= rows[i].TEMP %></td>
                <td><%= rows[i].BATT %></td>
              </tr>
              <% } %>
              --->
              <% rows.forEach(function(row) {%>
                <tr>
                  <td><a href="/sensing/<%= row.IDX %>"><%= row.IDX %></a></td>
                  <td><%= row.RSD %></td>
                  <td><a href="/sensing/read/<%= row.IA %>"><%= row.IA %></a></td>
                  <td><%= row.RSU %></td>
                  <td><%= row.SEQ %></td>
                  <td><%= row.REGDATE %></td>
                  <td><%= row.CO2 %></td>
                  <td><%= row.RH %></td>
                  <td><%= row.TEMP %></td>
                  <td><%= row.PM1P0 %></td>
                  <td><%= row.PM2P5 %></td>
                  <td><%= row.PM4P0 %></td>
                  <td><%= row.PM10P0 %></td>
                  <td><%= row.BATT %></td>
                </tr>
              <% }) %>
